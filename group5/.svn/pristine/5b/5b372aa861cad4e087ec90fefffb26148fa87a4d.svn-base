body{
    background-color: #181a1c;
	margin: 0 !important;
}
.rsvInfo,tr,th,td {
    border: 1px solid rgba(255, 255, 255, 0.1);
    margin: auto;
    padding-inline: 30px;
    text-align: center;
} 
.rsvInfo>thead>tr>th {
    background-color: rgba(255, 255, 255, 0.4);
    padding: 10px;
} 
.rsvInfo>tbody>tr>td {
    background-color: rgba(255, 255, 255, 0.1);
    padding: 30px;
    color : #fff;
}


.rsvSeat {
    border: none;
    border-spacing: 5px;
}       
.rsvSeat>tr,th,td {
    border: 1px solid rgba(255, 255, 255, 0.1) !important;
    margin: 10px;
    padding-inline: 10px;
    text-align: center;
}       
.rsvSeat>tbody>tr>td {
    padding: 10px;
    height: 20px;
    width: 20px;
}       
.rsvSeat>tbody>tr>td>a {
    color: #fff;
    text-decoration: none;
}        
.rsvSeat>tbody>tr>td:hover {
    background: #fece1a;
}

.footer-link>li {
    display: inline-block;
    padding-right: 20px;
}

.footer-link>li>a {
	color: #fece1a;
}

.footer-link>li>a:hover {
	font-weight: bolder;
}

.footer-link {
    text-align: left;
    list-style: none;
    font-size: smaller;
    margin-left: auto;
}

a {
    text-decoration: none !important;
}

.team-info {
    list-style: none;
    text-align: left;
    font-size: x-small;
    margin-left: auto;
}

.footer {
    background-color: #181a1c;
    float: left;
    width: 100%;
    height: 140px;
    line-height: 100%;
    padding-top: 26px;
}

.team-info {
	margin-top: 20px;
}

.footer-logo {
    width: 30%;
    height: 140px;
    float: left;
    text-align: right;
    line-height: 100%;
}

.footer-tools {
    float: right;
    width: 30%;
    height: 140px;
    text-align: left;
}

.btn{
	font-family: NanumSquareRoundR !important;
	font-weight: bolder !important;
}

.movieRank{
	float: left;
    width: 20%;
    text-align: center;
    margin-left: 80px;
}

#qnaListPage, #qnaRegisterPage, #aRegisterPage, #mypageView, #adminPage {
	text-align: center;
}

.qnaBackground {
	background: rgba(255,255,255,0.9);
	border: 1px solid rgba(255,255,255,0.7);
	border-radius: 10px;
	width: 1200px;
	display: inline-block;
	margin: 20px;
	padding: 10px;
}

.qnaBackground > table{
	width: 1160px;
	margin: 20px;
	margin-bottom: 100px;
	color: #181a1c;
}

.qnaBackground > table, th, td {
	border-bottom: 1px solid rgb(0, 0, 0, 0.1) !important;
}
	
.qnaBackground > table > thead, td {
	height : 30px;
	margin : 20px;
}

.qnaBackground > table > tbody > td > a:hover {
	color: #181a1c;
	font-weight: bolder !important;
}


.form-group > label {
	color: #181a1c;
	font-weight: bolder;
	display: inline-block;
	margin-left: -40px;
	margin-right: 20px;
	margin-top: 20px;
}

.form-group > input {
	border: 1px solid rgba(0,0,0,0.1);
	border-radius: 4px;
	padding-left: 20px;
	display: inline-block;
	height: 30px !important;
}

.qnaBackground > .row {
	width: 1200px;
}


.col-sm-6, .col-sm-12, .col-sm-8, .row, .form-group {
	display: inline-block !important;
	padding-left: 0 !important;
	padding-right: 0 !important;
	margin-left: 0 !important;
	margin-right: 0 !important;
	text-align: center;
}

.col-sm-6 > .form-group > input, option {
	width: 460px;
}

.col-sm-12 > .form-group > input {
	width: 1000px;
}

.col-sm-12 > .form-group > textarea {
	width: 800px;
}

.form-group > #content{
	width: 1000px !important;
}
.col-md-12{
	display: inline-block;
}

.mypageBackground {
	background: rgba(255,255,255,0.9);
	border: 1px solid rgba(255,255,255,0.7);
	border-radius: 10px;
	width: 1600px;
	display: inline-block;
	margin: 20px;
	padding: 10px;
}

.mypageBackground > table{
	width: 100%;
	background: rgba(255, 255, 255, 0.9);
}

.mypageTitle, .mypageBackground{
	width: 800px;
	display: inline-block;
}

.mypageTitle > h3, h4, a{
	display: inline;
	font-family: NanumSquareR !important;
	color: #fff !important;
}

.mypageTitle {
	text-align: left !important;
}


.mypageTitle > h4 {
	margin-left: 10px;
}


.mypageView > table > table, th, td, a {
	text-align: center !important;
	color: #181a1c;
}


.adminBackground {
	text-align: center;
	background: rgba(255,255,255,0.8);
	border: 1px solid rgba(255,255,255,0.9);
	border-radius: 10px;
	width: 1200px;
	display: inline-block;
	margin: 20px;
	padding: 10px;
}

#delBtnMemberListPage {
	width : 320px;
	heigth: 40px;
	margin-bottom : 20px;
}